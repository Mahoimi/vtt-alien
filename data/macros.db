{"name":"Alien - Roll on selected Creature table","permission":{"default":0,"vVRzTNcybBml63HZ":3,"q8HmJQOf882klbgH":3},"type":"script","sort":100001,"flags":{"furnace":{"runAsGM":false}},"scope":"global","command":"(async () => {\r\n    let options = \"\";\r\n    game.tables.entities.forEach(t => {\r\n        if (t.folder.name === \"Alien Creature Tables\"){\r\n                options = options.concat(`<option value=\"${t.data._id}\">${t.data.name}</option>`)\r\n            };\r\n    });\r\n    let template = `\r\n                    <form>\r\n                        <div class=\"form-group\">\r\n                            <label>Select Table</label>\r\n                            <select id=\"tableSelect\">${options}</select>\r\n                        </div>\r\n                        <div class=\"form-group\">\r\n                            <label>How Many?</label>\r\n                            <input type=\"text\" id=\"inputNbr\" value=1>\r\n                        </div>\r\n                    </form>`\r\n\r\n    let buttons = {}\r\n    if (game.tables.entities.length > 0) {\r\n        buttons = {\r\n            draw: {\r\n                icon: '<i class=\"fas fa-check\"></i>',\r\n                label: \"Draw\",\r\n                callback: async (html) => {\r\n                    const tableId = html.find('#tableSelect')[0].value;\r\n                    const table = game.tables.get(tableId);\r\n                    const drawNumber = parseInt(html.find('#inputNbr')[0].value || 0);\r\n                    for (let i = 0; i < drawNumber; i++) {\r\n                        await table.draw();\r\n                    };\r\n                }\r\n            },\r\n            cancel: {\r\n                icon: '<i class=\"fas fa-times\"></i>',\r\n                label: 'Cancel'\r\n            }\r\n        }\r\n    } else {\r\n        template = '<div style=\"text-align: center\">There are no tables to draw from!</div><br>'\r\n        buttons = {\r\n            draw: {\r\n                icon: '<i class=\"fas fa-check\"></i>',\r\n                label: \"OK\",\r\n            }\r\n        }\r\n    }\r\n\r\n    new Dialog({\r\n        title: 'Roll on selected Creature table',\r\n        content: template, buttons: buttons, default: \"draw\",\r\n    }).render(true);\r\n})()","author":"q8HmJQOf882klbgH","img":"icons/svg/dice-target.svg","actorIds":[],"_id":"3srykpXvL8jwsYf0"}
{"name":"Alien - Roll on selected Mother table","permission":{"default":0,"TLl1CBsB3Dponc04":3,"q8HmJQOf882klbgH":3},"type":"script","sort":100001,"flags":{"furnace":{"runAsGM":false}},"scope":"global","command":"(async () => {\n  let options = '';\n  game.tables.entities.forEach((t) => {\n    if (t.folder.name === 'Alien Mother Tables') {\n      options = options.concat(`<option value=\"${t.data._id}\">${t.data.name}</option>`);\n    }\n  });\n  let template = `\n                    <form>\n                        <div class=\"form-group\">\n                            <label>Select Table</label>\n                            <select id=\"tableSelect\">${options}</select>\n                        </div>\n                        <div class=\"form-group\">\n                            <label>How Many?</label>\n                            <input type=\"text\" id=\"inputNbr\" value=1>\n                        </div>\n                        <div class=\"form-group\">\n                            <label>Modifier?</label>\n                            <input type=\"text\" id=\"inputMod\" value=\"0\">\n                        </div>\n                    </form>`;\n\n  let buttons = {};\n  if (game.tables.entities.length > 0) {\n    buttons = {\n      draw: {\n        icon: '<i class=\"fas fa-check\"></i>',\n        label: 'Draw',\n        callback: async (html) => {\n          const tableId = html.find('#tableSelect')[0].value;\n          const table = game.tables.get(tableId);\n          const drawNumber = parseInt(html.find('#inputNbr')[0].value || 0);\n          const formula = table.data.formula;\n          const modifier = parseInt(html.find('#inputMod')[0].value || '0');\n\n          for (let i = 0; i < drawNumber; i++) {\n            const roll = new Roll(formula + ' + ' + modifier);\n            await table.draw({ roll: roll ,rollMode: \"blindroll\"});\n          }\n        },\n      },\n      cancel: {\n        icon: '<i class=\"fas fa-times\"></i>',\n        label: 'Cancel',\n      },\n    };\n  } else {\n    template = '<div style=\"text-align: center\">There are no tables to draw from!</div><br>';\n    buttons = {\n      draw: {\n        icon: '<i class=\"fas fa-check\"></i>',\n        label: 'OK',\n      },\n    };\n  }\n\n  new Dialog({\n    title: 'Roll on selected Mother table',\n    content: template,\n    buttons: buttons,\n    default: 'draw',\n  }).render(true);\n})();","author":"q8HmJQOf882klbgH","img":"icons/svg/biohazard.svg","actorIds":[],"_id":"6G8HUmxcuG97fmIS"}
